
<h2>All GaragePermission</h2>
<table class="table">
    <thead>
        <tr>
            <th>PermissionId</th>
            <th>UserId</th>
            <th>GarageId</th>
            <th>CanView</th>
            <th>CanEdit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var per in Model)
        {
            <tr>
                <td>@per.PermissionID</td>
                <td>@per.UserID</td>
                <td>@per.GarageID</td>
                <td>@per.CanView</td>
                <td>@per.CanEdit</td>
                <td>
                    <a href="@Url.Action("Update", "GaragePermission",new { id = @per.PermissionID})">Edit</a>

                </td>
            </tr>
        }
    </tbody>
</table>
